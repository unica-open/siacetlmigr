  <step>
    <name>Count mdp per mda</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Siac</connection>
    <sql>select a.accredito_tipo_code, a.accredito_tipo_desc , count(*) mdp from siac_t_modpag m, siac_d_accredito_tipo a 
where m.ente_proprietario_id = ${p_ente}
and m.accredito_tipo_id = a.accredito_tipo_id
and m.soggetto_id not in (select s01.soggetto_id from siac_t_soggetto s01 
                          where s01.ente_proprietario_id=${p_ente} and s01.login_operazione!=&apos;${p_login}&apos;)
group by a.accredito_tipo_code, a.accredito_tipo_desc
order by a.accredito_tipo_code</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
