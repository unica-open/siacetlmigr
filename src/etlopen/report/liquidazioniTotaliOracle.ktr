    <connection>siac_fonte</connection>
    <sql>&#47;*select ?,&apos;Oracle&apos; fonte, ${p_anno_esercizio} anno_esercizio,i.num_mov num_mov_i_fonte,i.importo importo,i.migr_importo i_migr_importo
from
(
select count(*) num_mov, sum(imp.importo) importo, sum(m.importo) migr_importo
from liquidazioni l,
     imp_liq imp,
     migr_liquidazione m 
where l.anno_esercizio=${p_anno_esercizio} and l.staoper in (&apos;P&apos;,&apos;D&apos;)
  and imp.anno_esercizio = l.anno_esercizio
  and imp.nliq = l.nliq
  and m.numero_liquidazione = imp.nliq
  and m.anno_esercizio = imp.anno_esercizio
  and m.numero_impegno= imp.nimp
  and m.numero_subimpegno=imp.nsubimp
  and m.anno_impegno=imp.annoimp
  and m.ente_proprietario_id=${p_ente})i*&#47;

&#47;*select ?,&apos;Oracle&apos; fonte, ${p_anno_esercizio} anno_esercizio ,i.num_mov num_mov_i_fonte,i.importo importo
       ,im.num_mov num_mov_inMigr
       ,im.importo i_migr_importo
from
(select count(*) num_mov, sum(imp.importo) importo
from liquidazioni l,
     imp_liq imp
where l.anno_esercizio=${p_anno_esercizio} and l.staoper in (&apos;P&apos;,&apos;D&apos;)
  and imp.anno_esercizio = l.anno_esercizio
  and imp.nliq = l.nliq)i
, 
(select count(*) num_mov, sum(m.importo) importo
from migr_liquidazione m 
where m.ente_proprietario_id=${p_ente} ) im*&#47;

select ?,&apos;Oracle&apos; fonte, ${p_anno_esercizio} anno_esercizio ,i.num_mov num_mov_i_fonte,i.importo importo
from
(select count(*) num_mov, sum(imp.importo) importo
