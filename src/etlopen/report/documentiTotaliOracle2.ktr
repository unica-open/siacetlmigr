           <schema_name/>
           </partitioning>
    <connection>siac_fonte</connection>
    <sql>select &apos;Oracle&apos; fonte, ${p_anno_esercizio} anno_esercizio,i.tipo_doc tipo_doc, i.num_mov num_mov_i_fonte,i.importo importo, i.ordine_tot
from
(
select 
 &apos;Entrata&apos; as tipo_doc, count(*) num_mov, sum(f.importo) importo, 1 as ordine_tot
   from migr_doc_entrata f where f.ente_proprietario_id=${p_ente}
            
union
select 
 &apos;Quote Entrata&apos; as tipo_doc, count(*) num_mov, sum(g.importo) importo, 2 as ordine_tot
from migr_docquo_entrata g where g.ente_proprietario_id=${p_ente}
            
union               
select 
 &apos;Spesa&apos; as tipo_doc, count(*) num_mov, sum(h.importo) importo, 3 as ordine_tot
 from migr_doc_spesa h where h.ente_proprietario_id=${p_ente} and h.tipo not in (&apos;ALG&apos;)
          
union
select 
 &apos;Quote Spesa&apos; as tipo_doc, count(*) num_mov, sum(l.importo) importo, 4 as ordine_tot    
from migr_docquo_spesa l where l.ente_proprietario_id=${p_ente} and l.tipo not in (&apos;ALG&apos;)
            
union       
select 
 &apos;Atti Allegati&apos; as tipo_doc, count(*) num_mov, 0 importo, 5 as ordine_tot 
from migr_atto_allegato m where m.ente_proprietario_id=${p_ente}
            
union
select 
 &apos;Atti Allegati Sogg.&apos; as tipo_doc, count(*) num_mov, 0 importo, 6 as ordine_tot 
from migr_atto_allegato_sog n where n.ente_proprietario_id=${p_ente}
          
union 
select 
 &apos;Elenco Doc. Allegati&apos; as tipo_doc, count(*) num_mov, 0 importo, 7 as ordine_tot
          from migr_elenco_doc_allegati o where o.ente_proprietario_id=${p_ente}
                 
union 
select 
 &apos;Relaz. Documenti&apos; as tipo_doc, count(*) num_mov, 0 importo, 8 as ordine_tot
from migr_relaz_documenti p where p.ente_proprietario_id=${p_ente}
          
)i
order by i.ordine_tot

</sql>
