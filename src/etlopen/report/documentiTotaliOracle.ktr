                 and migrLiq.sac_provvedimento=migrAA.Sac_Provvedimento
                 and migrLiq.ente_proprietario_id = migrAA.Ente_Proprietario_Id
                 and migrDocQuo.anno_esercizio=migrLiq.anno_esercizio
