                 and migrDocQuo.Ente_Proprietario_Id=migrLiq.Ente_Proprietario_Id
                 and migrLiq.anno_esercizio !=&apos;0000&apos;-- condizione che forza l&apos;uso dell&apos;indice XIFMIGR_DOCQUO_SPESA_LIQ sulla tabella migr_docquo_spesa
                 ) el1
