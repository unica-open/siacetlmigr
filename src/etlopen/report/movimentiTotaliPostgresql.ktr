           <schema_name/>
           </partitioning>
    <connection>Siac</connection>
    <sql>select &apos;PostGres&apos; fonte, ${p_anno_esercizio} anno_esercizio, i.num_mov num_mov_i_PG, i.imp_ini imp_ini, i2.imp_att imp_att, 
											 si.num_mov num_submov_i_PG,si.imp_ini simp_ini, si2.imp_att simp_att, 
											 a.num_mov num_mov_a_PG,a.imp_ini acc_ini, a2.imp_att acc_att, 
                                             sa.num_mov num_submov_a_PG,sa.imp_ini sacc_ini, sa2.imp_att sacc_att
from
(select count(*) num_mov,sum(mgd.movgest_ts_det_importo)imp_ini
from siac_t_movgest_ts_det mgd, siac_t_movgest_ts mg , siac_r_migr_impegno_movgest_ts r, migr_impegno m,
	 siac_d_movgest_ts_det_tipo ti
where m.ente_proprietario_id=${p_ente}
and m.tipo_movimento=&apos;I&apos;
and m.anno_esercizio=${p_anno_esercizio}::varchar
and m.migr_impegno_id=r.migr_impegno_id
and mg.movgest_ts_id = r.movgest_ts_id
and mgd.movgest_ts_id=mg.movgest_ts_id
and ti.ente_proprietario_id=${p_ente}
and ti.movgest_ts_det_tipo_code=&apos;I&apos; -- importo iniziale
and ti.data_cancellazione is null and
            date_trunc(&apos;day&apos;,now())&gt;=date_trunc(&apos;day&apos;,ti.validita_inizio) and
	        (date_trunc(&apos;day&apos;,now())&lt;=date_trunc(&apos;day&apos;,ti.validita_fine)or ti.validita_fine is null)
and mgd.movgest_ts_det_tipo_id=ti.movgest_ts_det_tipo_id)i
,(select sum(mgd.movgest_ts_det_importo) imp_att
from siac_t_movgest_ts_det mgd, siac_t_movgest_ts mg , siac_r_migr_impegno_movgest_ts r, migr_impegno m,
	 siac_d_movgest_ts_det_tipo ti
where m.ente_proprietario_id=${p_ente}
and m.tipo_movimento=&apos;I&apos;
and m.anno_esercizio=${p_anno_esercizio}::varchar
and m.migr_impegno_id=r.migr_impegno_id
and mg.movgest_ts_id = r.movgest_ts_id
and mgd.movgest_ts_id=mg.movgest_ts_id
and ti.ente_proprietario_id=${p_ente} 
and ti.movgest_ts_det_tipo_code=&apos;A&apos; -- importo attuale
and ti.data_cancellazione is null and
            date_trunc(&apos;day&apos;,now())&gt;=date_trunc(&apos;day&apos;,ti.validita_inizio) and
	        (date_trunc(&apos;day&apos;,now())&lt;=date_trunc(&apos;day&apos;,ti.validita_fine)or ti.validita_fine is null)
and mgd.movgest_ts_det_tipo_id=ti.movgest_ts_det_tipo_id) i2
,(select count(*) num_mov,sum(mgd.movgest_ts_det_importo)imp_ini
from siac_t_movgest_ts_det mgd, siac_t_movgest_ts mg , siac_r_migr_impegno_movgest_ts r, migr_impegno m,
	 siac_d_movgest_ts_det_tipo ti
where m.ente_proprietario_id=${p_ente}
and m.tipo_movimento=&apos;S&apos;
and m.anno_esercizio=${p_anno_esercizio}::varchar
and m.migr_impegno_id=r.migr_impegno_id
and mg.movgest_ts_id = r.movgest_ts_id
and mgd.movgest_ts_id=mg.movgest_ts_id
and ti.ente_proprietario_id=${p_ente} 
