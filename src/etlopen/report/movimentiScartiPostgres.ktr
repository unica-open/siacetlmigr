    <sql>select ?::Date,mA.movgest_anno anno_accertamento, mA.movgest_numero numero_accertamento,
       sum(ts.movgest_ts_importo) importo_vincolato,
       detAiniz.movgest_ts_det_importo importo_iniziale, detAAtt.movgest_ts_det_importo importo_attuale,
       deta.movgest_ts_det_importo importo_utilizzabile
from siac_r_movgest_ts ts,
     siac_t_movgest mA,siac_d_movgest_tipo tipoA ,
     siac_t_movgest_ts tsA,siac_d_movgest_ts_tipo tstipoA,
