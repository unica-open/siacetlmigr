  <step>
    <name>capitoli previsione ENTRATA</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>siac_fonte</connection>
    <sql>select &apos;3&apos; ente_proprietario_id_str,&apos;PVTO&apos; ente_denominazione,&apos;ORIGINE&apos; sistema,
       &apos;2&apos; ord, &apos;CAP-EP&apos; tipo_capitolo,to_char(prev_peg.numero) numero,
       prev_peg.st_iniziale st_iniziale,
       prev_peg.residuo+prev_peg.residuo_80000 st_iniziale_res,
       prev_peg.st_iniziale+prev_peg.residuo+prev_peg.residuo_80000 st_iniziale_cassa,
       prev_peg.stanziamento stanziamento,
       prev_peg.residuo+prev_peg.residuo_80000 stanziamento_res,
       prev_peg.stanziamento+prev_peg.residuo+prev_peg.residuo_80000 stanziamento_cassa,
       prev_peg.st_iniziale_anno2 st_iniziale_anno2, prev_peg.stanziamento_anno2 stanziamento_anno2,
       prev_peg.st_iniziale_anno3 st_iniziale_anno3, prev_peg.stanziamento_anno3 stanziamento_anno3
from 
(select count(*) numero,
        sum(cAnno.importo) st_iniziale,
        nvl(sum(IMPEGNATO.residui_da_riportare),0) residuo, 
        nvl(sum(IMPEGNATO_80000.residuo),0) residuo_80000,
        sum(cAnno.importo) stanziamento, 
        sum(cAnno2.importo) st_iniziale_anno2, sum(cAnno2.importo) stanziamento_anno2,
        sum(cAnno3.importo) st_iniziale_anno3, sum(cAnno3.importo) stanziamento_anno3
from prev_peg cAnno, prev_peg cAnno2, prev_peg cAnno3,
     d118_prev_ent cap118,
   (select res.nro_capitolo nro_capitolo, nvl(sum(res.residui_da_riportare ),0) residui_da_riportare
      from  as_capitoli res
      where res.anno_peg=to_number(&apos;${p_anno_esercizio}&apos;)-1 and
            res.anno_capitolo&lt;=res.anno_peg and
            res.tipo_eu=&apos;E&apos; 
    group by res.nro_capitolo) IMPEGNATO,
     (select res.nro_capitolo nro_capitolo, nvl(sum(res.residuo ),0) residuo
      from  residui_80000 res
      where res.anno_capitolo=&apos;${p_anno_esercizio}&apos; and
            res.tipo_eu=&apos;E&apos;
      group by res.nro_capitolo) IMPEGNATO_80000
where cAnno.anno_ese=to_number(&apos;${p_anno_esercizio}&apos;)-1 and
      cAnno.Anno_Capitolo_77=&apos;${p_anno_esercizio}&apos; and
      cAnno.Tipo_Int_77   = &apos;E&apos; and
      cAnno2.anno_ese (+) = cAnno.anno_ese and
      cAnno2.Anno_Capitolo_77 (+) = to_number(cAnno.Anno_Capitolo_77)+1 and
      cAnno2.Tipo_Int_77 (+) = cAnno.Tipo_Int_77 and       
      cAnno2.nro_capitolo_77 (+) = cAnno.nro_capitolo_77 and
      cAnno3.anno_ese (+) = cAnno.anno_ese and
      cAnno3.Anno_Capitolo_77 (+) = to_number(cAnno.Anno_Capitolo_77)+2 and
      cAnno3.Tipo_Int_77 (+) = cAnno.Tipo_Int_77 and       
      cAnno3.nro_capitolo_77 (+) = cAnno.nro_capitolo_77 and
      cap118.anno_esercizio  (+) = cAnno.anno_capitolo_77 and
      cap118.anno_capitolo   (+) = cAnno.anno_capitolo_77 and
      cap118.nro_capitolo    (+) = cAnno.nro_capitolo_77 and
      cap118.nro_articolo    (+) = 0 and
      IMPEGNATO.nro_capitolo (+) = cAnno.nro_capitolo_77 and
      IMPEGNATO_80000.nro_capitolo (+) = cAnno.nro_capitolo_77 
) prev_peg</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
