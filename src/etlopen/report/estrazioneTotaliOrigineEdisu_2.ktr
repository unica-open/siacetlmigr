    <connection>siac_fonte</connection>
    <sql>select &apos;${p_ente}&apos; ente_proprietario_id_str,e.nome ente_denominazione,&apos;ORIGINE&apos; sistema,
       &apos;4&apos; ord, &apos;CAP-EG&apos; tipo_capitolo,to_char(count(*)) numero,
       sum(cAnno.St_attu) st_iniziale, sum(cAnno.st_attu_res) st_iniziale_res,sum(cAnno.st_attu_cassa) st_iniziale_cassa,
       sum(cAnno.St_attu) stanziamento, sum(cAnno.st_attu_res) stanziamento_res,sum(cAnno.st_attu_cassa) stanziamento_cassa,
       sum( cAnno2.st_attu) st_iniziale_anno2,sum(cAnno2.st_attu) stanziamento_anno2,
       sum(cAnno3.st_attu) st_iniziale_anno3,sum(cAnno3.st_attu) stanziamento_anno3
from cap_entrata cAnno, cap_entrata cAnno2,cap_entrata cAnno3, d118_cap_ent cap118,
     ente e
where      cAnno.Anno_Esercizio=&apos;${p_anno_esercizio}&apos; and
           cAnno.nro_capitolo&gt;=0 and 
           cAnno2.anno_esercizio=to_number(cAnno.anno_esercizio)+1 and
           cAnno2.nro_capitolo = cAnno.nro_capitolo and
           cAnno2.nro_articolo= cAnno.nro_articolo and
           cAnno3.anno_esercizio=to_number(cAnno.anno_esercizio)+2 and
           cAnno3.nro_capitolo = cAnno.nro_capitolo and
           cAnno3.nro_articolo= cAnno.nro_articolo and
           cap118.anno_esercizio=cAnno.anno_esercizio and
           cap118.nro_capitolo=cAnno.nro_capitolo and
           cap118.nro_articolo=cAnno.nro_articolo
group by e.nome</sql>
