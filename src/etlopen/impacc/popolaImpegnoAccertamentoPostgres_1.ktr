(migr_tipo,migr_tipo_elab,esito,messaggio_esito,data_creazione,ente_proprietario_id)
(select  &apos;?&apos;,&apos;?&apos;,(case  when migrImpAccert.numerorecordinseriti=-1 then &apos;KO&apos;
						when migrImpAccert.numerorecordinseriti=-13 then &apos;KO&apos;
                                                              else &apos;OK&apos; END),migrImpAccert.messaggioRisultato,CURRENT_TIMESTAMP,?
 from fnc_aggiorna_accertamento_vincolato (?,&apos;?&apos;,&apos;?&apos;,&apos;?&apos;)  migrImpAccert );

analyze migr_impegno_accertamento;
