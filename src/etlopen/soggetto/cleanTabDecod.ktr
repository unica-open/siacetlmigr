    <sql>  DELETE FROM siac_t_forma_giuridica t where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;
	and not exists (select 1 from siac_r_forma_giuridica r  where t.forma_giuridica_id=r.forma_giuridica_id)
	and not exists (select 1 from siac_r_forma_giuridica_mod r  where t.forma_giuridica_id=r.forma_giuridica_id);
  DELETE FROM siac_r_comune_provincia where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_t_provincia where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_t_comune where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_t_nazione where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
-- 27.07.2016 Sofia aggiunta cancellazione di questa tabella di relazione per cancellazione della successiva
  DELETE FROM siac_r_movgest_ts_sogclasse where ente_proprietario_id = ${p_ente} 
  and soggetto_classe_id in (select soggetto_classe_id FROM siac_d_soggetto_classe where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;);
  DELETE FROM siac_d_soggetto_classe where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_r_migr_classe_soggclasse where ente_proprietario_id = ${p_ente};
-- Dani 19.02.2016 Delete per accredito tipo che sar√† cancellato subito dopo
  DELETE FROM siac_r_accredito_tipo_cassa_econ r using siac_d_accredito_tipo d
	where r.ente_proprietario_id = ${p_ente}
    and r.accredito_tipo_id = d.accredito_tipo_id and d.ente_proprietario_id = ${p_ente} and d.login_operazione = &apos;${p_login_operazione}&apos;;
-- Dani 03.03.2016 Delete per accredito tipo
  DELETE FROM siac_r_accredito_tipo_oil r using siac_d_accredito_tipo d
	where r.ente_proprietario_id = ${p_ente}
    and r.accredito_tipo_id = d.accredito_tipo_id and d.ente_proprietario_id = ${p_ente} and d.login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_d_accredito_tipo where ente_proprietario_id = ${p_ente} and login_operazione = &apos;${p_login_operazione}&apos;;
  DELETE FROM siac_r_migr_mod_accredito_accredito where ente_proprietario_id = ${p_ente};

